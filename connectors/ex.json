{
  "connector.class": "io.debezium.connector.mysql.MySqlConnector",
  "tasks.max": "1",
  "database.allowPublicKeyRetrieval": "true",
  "key.converter": "io.confluent.connect.avro.AvroConverter",
  "value.converter": "io.confluent.connect.avro.AvroConverter",
  "transforms": "convertTimezone, unwrap",
  "transforms.convertTimezone.type": "io.debezium.transforms.TimezoneConverter",
  "transforms.convertTimezone.converted.timezone": "+09:00",
  "transforms.convertTimezone.exclude.list": "source:tr_et:cbz_dt, tr_et:fshp_dt",
  "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
  "transforms.unwrap.delete.handling.mode": "rewrite",
  "transforms.unwrap.add.fields": "op",
  "transforms.unwrap.drop.tombstones": "false",

  "topic.prefix": "tst.tstdb",
  "database.hostname": "mysql",
  "database.port": "3306",
  "database.user": "root",
  "database.password": "root_pw123!",
  "database.server.id": "196101",
  "database.ssl.mode": "disabled",
  "snapshot.mode": "schema_only",
  "snapshot.locking.mode": "none",
  "time.precision.mode": "connect",
  "heartbeat.interval.ms": "3000",
  "table.include.list": "tstdb.st_inl_user, tstdb.tr_et",
  "database.include.list": "tstdb",
  "errors.tolerance": "all",
  "errors.log.enable": "true",
  "errors.log.include.messages": "true",
  "errors.deadletterqueue.context.headers.enable": "true",
  "errors.deadletterqueue.topic.name": "dlq.tstdb",
  "schema.history.internal.kafka.topic": "schemahistory.tstdb",
  "schema.history.internal.kafka.bootstrap.servers": "broker:29092",
  "key.converter.schema.registry.url": "http://schema-registry:8081",
  "value.converter.schema.registry.url": "http://schema-registry:8081"
}


{
  "connector.class": "io.confluent.connect.jdbc.JdbcSinkConnector",
  "connection.password": "myuser_pw123!",
  "tasks.max": "1",
  "max.retries": "1",
  "table.types": "TABLE",
  "auto.evolve": "false",
  "db.timezone": "Asia/Seoul",
  "value.converter": "io.confluent.connect.avro.AvroConverter",
  "insert.mode": "upsert",
  "key.converter": "io.confluent.connect.avro.AvroConverter",
  "dialect.name": "MySqlDatabaseDialect",
  "table.name.format": "tstdb.st_inl_user_out",
  "topics": "tst.tstdb.tstdb.st_inl_user",
  "schema.history.internal.kafka.bootstrap.servers": "broker:29092",
  "fields.whitelist": "user_id, st_dvs_cd, pwd_lin_err_ntm, use_yn, user_cnts, regr_no, reg_dttm, modr_no,
mod_dttm",
  "value.converter.schema.registry.url": "http://schema-registry:8081",
  "connection.user": "myuser",
  "trim.sensitive.log": "true",
  "auto.create": "false",
  "connection.url": "jdbc:mysql://mysql:3306/tstdb",
  "pk.mode": "record_value",
  "key.converter.schema.registry.url": "http://schema-registry:8081",
  "pk.fields": "user_no",
  "mssql.use.merge.holdlock": "true"
